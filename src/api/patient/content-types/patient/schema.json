{
  "kind": "collectionType",
  "collectionName": "patients",
  "info": {
    "singularName": "patient",
    "pluralName": "patients",
    "displayName": "Patient",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "mobileNo": {
      "type": "biginteger",
      "max": "9999999999",
      "min": "1000000000"
    },
    "dateOfBirth": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female",
        "Others"
      ],
      "required": true
    },
    "doctor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
